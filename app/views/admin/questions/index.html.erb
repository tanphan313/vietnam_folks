<h1>Danh sách câu hỏi</h1>
<h3>Category: <%= link_to "#{@category.title}", edit_admin_category_path(@category) %></h3>

<%= link_to "Tạo câu hỏi", new_admin_category_question_path(@category), class: "btn btn-primary" %>

<table class="table table-hover">
  <thead>
    <tr>
      <th>#</th>
      <th>Content</th>
      <th>Option</th>
    </tr>
  </thead>
  <tbody>
    <%= render @questions %>
  </tbody>
</table>
<%= will_paginate @questions %>
